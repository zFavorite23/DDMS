ength=\"20\"\r\n                        show-word-limit\r\n                    ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"公司注册地址\" prop=\"address\">\r\n                    <el-input\r\n                        type=\"text\"\r\n                        placeholder=\"请输入公司注册地址\"\r\n                        v-model=\"formData.address\"\r\n                        auto-complete=\"off\"\r\n                        maxlength=\"50\"\r\n                        show-word-limit\r\n                    ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"公司电话\" prop=\"telephone\">\r\n                    <el-input\r\n                        type=\"text\"\r\n                        placeholder=\"请输入公司电话\"\r\n                        v-model=\"formData.telephone\"\r\n                        auto-complete=\"off\"\r\n                        maxlength=\"20\"\r\n                        show-word-limit\r\n                    ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"税号\" prop=\"dutyParagraph\">\r\n                    <el-input\r\n                        type=\"text\"\r\n                        placeholder=\"请输入税号\"\r\n                        v-model=\"formData.dutyParagraph\"\r\n                        auto-complete=\"off\"\r\n                        maxlength=\"20\"\r\n                        show-word-limit\r\n                    ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"开户行名称\" prop=\"bankName\">\r\n                    <el-input\r\n                        type=\"text\"\r\n                        placeholder=\"请输入开户行名称\"\r\n                        v-model=\"formData.bankName\"\r\n                        auto-complete=\"off\"\r\n                        maxlength=\"20\"\r\n                        show-word-limit\r\n                    ></el-input>\r\n                </el-form-item>\r\n                <el-form-item labe