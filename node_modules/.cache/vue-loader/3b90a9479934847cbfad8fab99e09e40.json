CA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"List.vue","sourceRoot":"src/views/purchase/purchaseApply","sourcesContent":["<template>\r\n    <div>\r\n        <div class=\"\">\r\n            <span class=\"tit\">采购 / 总数 : {{ total }}</span>\r\n            <el-form :inline=\"true\" :model=\"query\">\r\n                <el-form-item>\r\n                    <el-select style=\"width:120px\" :disabled=\"disabled\" v-model=\"query.userId\" filterable placeholder=\"请选择\">\r\n                        <el-option v-for=\"item in userOptions\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\" :disabled=\"item.disabled\"></el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item><el-input style=\"width:120px\" v-model=\"query.likeKeyWords\" placeholder=\"关键字\" clearable></el-input></el-form-item>\r\n                <el-form-item>\r\n                    <el-select style=\"width:120px\" clearable v-model=\"query.status\" placeholder=\"请选择\">\r\n                        <el-option v-for=\"item in statusOptions\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\" :disabled=\"item.disabled\"></el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item>\r\n                    <el-select style=\"width:120px\" clearable v-model=\"query.classify\" placeholder=\"总分类\">\r\n                        <el-option v-for=\"item in classifyOptions\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\" :disabled=\"item.disabled\"></el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item><el-button type=\"primary\" size=\"medium\" icon=\"el-icon-search\">搜索</el-button></el-form-item>\r\n                <el-form-item>\r\n                    <router-link to=\"/purchase/purchaseApply/form\"><el-button type=\"primary\" size=\"medium\">添加申请</el-button></router-link>\r\n                </el-form-item>\r\n            </el-form>\r\n            <el-radio-group v-model=\"listType\" style=\"float: right;\" @change=\"openList\">\r\n                <el-radio-button label=\"1\">我申请的</el-radio-button>\r\n                <el-radio-button label=\"2\">我审批的</el-radio-button>\r\n            </el-radio-group>\r\n        </div>\r\n\r\n        <el-table :data=\"list\" stripe border v-loading=\"listLoading\" style=\"width: 100%;\" :default-sort=\"{ prop: 'createTime', order: 'descending' }\">\r\n            <el-table-column width=\"50\" label=\"序号\">\r\n                <template scope=\"scope\">\r\n                    <span>{{ scope.$index + (query.current - 1) * query.size + 1 }}</span>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column width=\"80\" label=\"申请人\">\r\n                <template scope=\"scope\">\r\n                    <span>{{ scope.row.applyUserName }}</span>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column width=\"110\" label=\"采购物品名称\"><template slot-scope=\"scope\"></template></el-table-column>\r\n            <el-table-column width=\"110\" label=\"分类\">\r\n                <template slot-scope=\"scope\">\r\n                    <p>{{ scope.row.categoryName }}</p>\r\n                    <p>{{ scope.row.subClassifyName }}</p>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column width=\"80\" label=\"申请时间\"><template slot-scope=\"scope\"></template></el-table-column>\r\n            <el-table-column width=\"100\" label=\"淘宝链接\"><template slot-scope=\"scope\"></template></el-table-column>\r\n            <el-table-column width=\"160\" label=\"型号参数\"><template slot-scope=\"scope\"></template></el-table-column>\r\n            <el-table-column width=\"50\" label=\"数量\"><template slot-scope=\"scope\"></template></el-table-column>\r\n            <el-table-column width=\"50\" label=\"单位\"><template slot-scope=\"scope\"></template></el-table-column>\r\n            <el-table-column width=\"50\" label=\"单价\"><template slot-scope=\"scope\"></template></el-table-column>\r\n            <el-table-column width=\"50\" label=\"总价\"><template slot-scope=\"scope\"></template></el-table-column>\r\n            <el-table-column width=\"150\" label=\"收货地址,联系方式\"><template slot-scope=\"scope\"></template></el-table-column>\r\n            <el-table-column width=\"150\" label=\"审批人\">\r\n                <template slot-scope=\"scope\">\r\n                    <div class=\"tag-group\" v-for=\"item in scope.row.checkUserList\">\r\n                        <p>{{ item.checkUserName }}</p>\r\n                        <el-tag v-if=\"item.check == 0 && item.isBeing == 1\" size=\"mini\" type=\"\">审批中</el-tag>\r\n                        <el-tag v-if=\"item.check == 0 && item.isBeing == 0\" size=\"mini\" type=\"warning\">待审批</el-tag>\r\n                        <el-tag v-if=\"item.check == 1\" size=\"mini\" type=\"success\">已同意</el-tag>\r\n                        <el-tag v-if=\"item.check == 2\" size=\"mini\" type=\"danger\">已拒绝</el-tag>\r\n                    </div>\r\n                </template>\r\n            </el-table-column>\r\n\r\n            <el-table-column prop=\"status\" width=\"100\" label=\"审批状态\">\r\n                <template slot-scope=\"scope\">\r\n                    <el-tag v-if=\"scope.row.status == 0\" type=\"warning\">审批中</el-tag>\r\n                    <el-tag v-if=\"scope.row.status == 1\" type=\"success\">同意</el-tag>\r\n                    <el-tag v-if=\"scope.row.status == 2\" type=\"danger\">拒绝</el-tag>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column width=\"100\" label=\"采购负责人\"><template slot-scope=\"scope\"></template></el-table-column>\r\n            <el-table-column width=\"120\" label=\"物流信息\"><template slot-scope=\"scope\"></template></el-table-column>\r\n            <el-table-column label=\"操作\" min-width=\"180\" fixed=\"right\">\r\n                <template slot-scope=\"scope\">\r\n                    <el-button size=\"mini\" type=\"primary\" @click.native=\"lookModal(scope.row)\">查看</el-button>\r\n                    <el-button size=\"mini\" v-if=\"scope.row.status != 1\" @click.native=\"handleModal(scope.row)\">编辑</el-button>\r\n                    <el-button size=\"mini\" v-if=\"scope.row.status == 0\" type=\"danger\" @click.native=\"handleDel(scope.$index, scope.row)\">删除</el-button>\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n        <el-pagination\r\n            class=\"right\"\r\n            @size-change=\"handleSizeChange\"\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page.sync=\"query.current\"\r\n            :page-sizes=\"[10, 20, 50, 100]\"\r\n            :page-size=\"query.size\"\r\n            layout=\"total, sizes, prev, pager, next\"\r\n            :total=\"total\"\r\n        ></el-pagination>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { getUserList } from '../../../api/admin/user.js';\r\nimport { mapGetters } from 'vuex';\r\nexport default {\r\n    data() {\r\n        return {\r\n            query: {\r\n                userId: null,\r\n                status: '',\r\n                likeKeyWords: '',\r\n                current: 1,\r\n                size: 10\r\n            },\r\n            list: [],\r\n            listType: '1',\r\n            pages: 0,\r\n            total: 0,\r\n            disabled: false,\r\n            listLoading: false,\r\n            statusOptions: [\r\n                {\r\n                    value: '',\r\n                    label: '全部'\r\n                },\r\n                {\r\n                    value: '0',\r\n                    label: '审批中'\r\n                },\r\n                {\r\n                    value: '1',\r\n                    label: '同意'\r\n                },\r\n                {\r\n                    value: '2',\r\n                    label: '拒绝'\r\n                }\r\n            ],\r\n            classifyOptions: [\r\n                {\r\n                    value: '',\r\n                    label: '全部'\r\n                }\r\n            ],\r\n            userOptions: []\r\n        };\r\n    },\r\n    created() {\r\n        this.query.userId = this.userId;\r\n        this.getUserList();\r\n    },\r\n    computed: {\r\n        ...mapGetters(['permissions', 'userId'])\r\n    },\r\n    methods: {\r\n        openList(val) {\r\n            if (val == 2) {\r\n                this.$router.push({\r\n                    path: '/purchase/purchaseApply/approver'\r\n                });\r\n            }\r\n        },\r\n\r\n        getUserList() {\r\n            getUserList().then(response => {\r\n                console.log(response);\r\n                response.data.data.forEach(element => {\r\n                    //console.log(element)\r\n                    this.userOptions.push({\r\n                        value: element.userId,\r\n                        label: element.username\r\n                    });\r\n                });\r\n            });\r\n        },\r\n\r\n        handleSizeChange(val) {\r\n            this.query.size = val;\r\n            this.getInvoicePage();\r\n        },\r\n        handleCurrentChange(val) {\r\n            this.query.current = val;\r\n            sessionStorage.setItem('page2', val);\r\n            this.getInvoicePage();\r\n        }\r\n    }\r\n    // computed: {\r\n    //     ...mapGetters(['permissions', 'userId'])\r\n    // }\r\n};\r\n</script>\r\n\r\n<style></style>\n"]}]}