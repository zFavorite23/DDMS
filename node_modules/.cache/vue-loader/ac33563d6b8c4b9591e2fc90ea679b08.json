sponse.data.data.size;\r\n                    this.list = response.data.data.records;\r\n                })\r\n                .catch(() => {\r\n                    this.listLoading = false;\r\n                });\r\n        },\r\n        handleModal(data) {\r\n            if (data.itemId) {\r\n                window.localStorage.setItem(\r\n                    \"editProjectInfo\",\r\n                    JSON.stringify(data)\r\n                );\r\n                this.$router.push({\r\n                    path: \"/project/itemForm\"\r\n                });\r\n            } else {\r\n                this.$router.push({\r\n                    path: \"/project/itemForm\"\r\n                });\r\n            }\r\n        },\r\n        handleModalInfo(data) {\r\n            window.localStorage.setItem(\"itemInfo\", JSON.stringify(data));\r\n            this.$router.push({\r\n                path: \"/project/itemInfo/\" + data.itemId\r\n            });\r\n        },\r\n        handleSizeChange(val) {\r\n            this.query.size = val;\r\n            this.getItemList();\r\n        },\r\n        handleCurrentChange(val) {\r\n            this.query.current = val;\r\n            this.getItemList();\r\n        },\r\n        // 删除\r\n        handleDel(index, row) {\r\n            if (row.itemId) {\r\n                this.$confirm(\"确认删除 \" + row.alias + \" 吗?\", \"提示\", {\r\n                    type: \"warning\"\r\n                }).then(() => {\r\n                    deleteItem(row.itemId).then(res => {\r\n                        if (res.data.data) {\r\n                            this.getItemList();\r\n                        }\r\n                    });\r\n                });\r\n            }\r\n        },\r\n        handleTeam(row) {}\r\n    },\r\n    mounted() {}\r\n};\r\n</script>\r\n<style type=\"text/scss\" scoped lang=\"scss\">\r\n.el-input .el-input__count .el-input__count-inner {\r\n    height: 50%;\r\n}\r\n@media (min-width: 600px) {\r\n    .zzz {\r\n        d