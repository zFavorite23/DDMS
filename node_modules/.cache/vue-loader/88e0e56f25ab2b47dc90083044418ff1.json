\n        <div class=\"\">\r\n            <span class=\"tit\">报销 / 总数 : {{ total }}</span>\r\n            <el-form :inline=\"true\" :model=\"query\">\r\n                <el-form-item>\r\n                    <el-select style=\"width:120px\" :disabled=\"disabled\" v-model=\"query.userId\" filterable placeholder=\"请选择\">\r\n                        <el-option v-for=\"item in userOptions\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\" :disabled=\"item.disabled\"></el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item><el-input style=\"width:120px\" v-model=\"query.likeKeyWords\" placeholder=\"关键字\" clearable></el-input></el-form-item>\r\n                <el-form-item>\r\n                    <el-select style=\"width:120px\" clearable v-model=\"query.status\" placeholder=\"请选择\">\r\n                        <el-option v-for=\"item in statusOptions\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\" :disabled=\"item.disabled\"></el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item>\r\n                    <el-select style=\"width:120px\" clearable v-model=\"query.classify\" placeholder=\"总分类\">\r\n                        <el-option v-for=\"item in classifyOptions\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\" :disabled=\"item.disabled\"></el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n                <!-- <el-form-item>\r\n                    <el-select style=\"width:140px\" v-model=\"query.orderBy\" placeholder=\"排序方式\">\r\n                        <el-option v-for=\"item in orderByOptions\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\" :disabled=\"item.disabled\"></el-option>\r\n                    </el-select>\r\n                </el-form-item> -->\r\n                <el-form-item><el-button type=\"primary\" size=\"medium\" v-on:click=\"getInvoicePage()\" icon=\"el-icon-search\">搜索</el-button></el-form-item>\r\n                <el-form-item>\r\n                    <router-link to=\"/apply/invoice/form\"><el-button type=\"primary\" size=\"medium\" @change=\"clearCon\">添加申请</el-button></router-link>\r\n                </el-form-item>\r\n            </el-form>\r\n            <el-radio-group v-model=\"listType\" style=\"float: right;\" @change=\"openList\">\r\n                <el-radio-button label=\"1\">我申请的</el-radio-button>\r\n                <el-radio-button label=\"2\">我审批的</el-radio-button>\r\n            </el-radio-group>\r\n        </div>\r\n        <el-table :data=\"list\" stripe border v-loading=\"listLoading\" style=\"width: 100%;\" :default-sort=\"{ prop: 'createTime', order: 'descending' }\">\r\n            <el-table-column width=\"80\" label=\"序号\">\r\n                <template scope=\"scope\">\r\n                    <span>{{ scope.$index + (query.current - 1) * query.size + 1 }}</span>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column width=\"100\" label=\"申请人\">\r\n                <template scope=\"scope\">\r\n                    <span>{{ scope.row.applyUserName }}</span>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column min-width=\"160\" label=\"分类\">\r\n                <template slot-scope=\"scope\">\r\n                    <p v-if=\"scope.row.classify == 7\">办公用品</p>\r\n                    <p v-else-if=\"scope.row.classify == 8\">固定资产</p>\r\n                    <p v-else-if=\"scope.row.classify == 9\">管理费用</p>\r\n                    <p v-else-if=\"scope.row.classify == 10\">人员补助</p>\r\n                    <p v-else-if=\"scope.row.classify == 11\">项目报销</p>\r\n                    <p v-else-if=\"scope.row.classify == 12\">产品报销</p>\r\n\r\n                    <p v-if=\"scope.row.classify == 7 && scope.row.type == 1\">办公文具</p>\r\n                    <p v-else-if=\"scope.row.classify == 7 && scope.row.type == 2\">办公耗材</p>\r\n                    <p v-else-if=\"scope.row.classify == 7 && scope.row.type == 3\">日杂百货</p>\r\n                    <p v-else-if=\"scope.row.classify == 7 && scope.row.type == 4\">财务用品v</p>\r\n                    <p v-else-if=\"scope.row.classify == 7 && scope.row.type == 5\">电子设备及工具</p>\r\n\r\n                    <p v-else-if=\"scope.row.classify == 8 && scope.row.type == 1\">办公设备</p>\r\n                    <p v-else-if=\"scope.row.classify == 8 && scope.row.type == 2\">办公家具</p>\r\n\r\n                    <p v-else-if=\"scope.row.classify == 9 && scope.row.type == 1\">市内交通</p>\r\n                    <p v-else-if=\"scope.row.classify == 9 && scope.row.type == 2\">办公费用</p>\r\n                    <p v-else-if=\"scope.row.classify == 9 && scope.row.type == 3\">运输</p>\r\n                    <p v-else-if=\"scope.row.classify == 9 && scope.row.type == 4\">业务招待费</p>\r\n                    <p v-else-if=\"scope.row.classify == 9 && scope.row.type == 5\">会议费</p>\r\n                    <p v-else-if=\"scope.row.classify == 9 && scope.row.type == 6\">差旅费</p>\r\n                    <p v-else-if=\"scope.row.classify == 9 && scope.row.type == 7\">福利费</p>\r\n\r\n                    <p v-else-if=\"scope.row.classify == 10 && scope.row.type == 1\">住房补助</p>\r\n\r\n                    <p v-else-if=\"(scope.row.classify == 11 || scope.row.classify == 12) && scope.row.type == 1\">差旅费</p>\r\n                    <p v-else-if=\"(scope.row.classify == 11 || scope.row.classify == 12) && scope.row.type == 2\">业务招待费</p>\r\n                    <p v-else-if=\"(scope.row.classify == 11 || scope.row.classify == 12) && scope.row.type == 3\">外协</p>\r\n                    <p v-else-if=\"(scope.row.classify == 11 || scope.row.classify == 12) && scope.row.type == 4\">设备采购</p>\r\n                    <p v-else-if=\"(scope.row.classify == 11 || scope.row.classify == 12) && scope.row.type == 5\">办公费</p>\r\n                    <p v-else-if=\"(scope.row.classify == 11 || scope.row.classify == 1